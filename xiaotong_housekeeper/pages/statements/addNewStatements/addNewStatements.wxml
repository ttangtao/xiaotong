<view class='flex otherOptions'>
  <text style='color:#30c37d'>该外包公司总产值(元)</text>
  <text style='color:#30c37d'>0</text>
</view>
<view class='flex otherOptions'>
  <text style='color:#30c37d'>该外包公司总结算单(元)</text>
  <text style='color:#30c37d'>0</text>
</view>
<view class='flex otherOptions'>
  <text style='color:#30c37d'>总产值-总结算单(元)</text>
  <text style='color:#30c37d'>0</text>
</view>

<view class='flex otherOptions'>
<view><text>外包公司名称</text></view>
  <picker value="{{waibaoname}}" range='{{waibaonameAry}}'  bindchange="bindDateChange">
    <view class="picker flex">
      <view><text>{{waibaoname}}</text></view>
      <view class='icon2'><image src='../../img/bottom.png'></image></view>
    </view>
  </picker>
</view>

<view class='flex otherOptions'>
<view><text>所属月份</text></view>
  <picker mode="date" fields="month" value="{{date}}"  bindchange="bindDateChange">
    <view class="picker flex">
      <view><text>{{date}}</text></view>
      <view class='icon2'><image src='../../img/bottom.png'></image></view>
    </view>
  </picker>
</view>

<view>
  <view wx:for="{{row2List}}" wx:key="{{index}}" class='flex row otherRow'>
    <text>{{item.key}}</text>
    <text>{{item.value}}</text>
  </view>
</view>

<view>
  <view class='flex row otherRow'>
    <view>
      <text>订单照片</text>
    </view>
    <view class='icon2'>
      <image src='../../img/add.png'></image>
    </view>
  </view>
</view>

<view style='padding:10rpx;' class='flex'>
  <view class='icon1'>
    <image src='../../img/start2.png'></image>
  </view>
  <view style='margin-left:10rpx;'>
    <text>结算明细</text>
  </view>
</view>

<view>
  <view wx:for="{{row3List}}" wx:key="{{index}}" class='flex row otherRow'>
    <text>{{item.key}}</text>
    <text>{{item.value}}</text>
  </view>
</view>

<view style='height:180rpx'></view>
<view bindtap='commitBtn' class='commitBtn'>
  <button class='ptButton' style='border-radius:0' hover-class='ptButtonHover'>提交</button>
</view>