<!--pages/ProjectInformation/OutputValue/OutputValue.wxml-->

<view class='rankHead flex'>
  <view class='rankPick flex'>
    <view class="section__title">日期</view>
    <picker mode="date" value="{{rankDate}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange">
      <view class="picker flex">
        <view>
          <text>{{rankDate}}</text>
        </view>
        <view class='rankIcon'>
          <image src='../img/bottom.png'></image>
        </view>
      </view>
    </picker>
  </view>
  <view class='rankPick flex'>
    <view class="section__title">类型</view>
    <picker value="{{rankType}}" range="{{rankTypeAry}}" bindchange="bindTypeChange">
      <view class="picker flex">
        <view>
          <text>{{rankType}}</text>
        </view>
        <view class='rankIcon'>
          <image src='../img/bottom.png'></image>
        </view>
      </view>
    </picker>
  </view>
</view>

<view wx:for="{{5}}" wx:key="{{index}}" data-kind='row' bindtap='jump' class='flex row weui-check__label'>
  <view class='icon2 otherOptions'>
    <image src='../img/order.png'></image>
  </view>
  <view style='flex-grow:1;margin-left:10rpx'>
    <view>
      <text>订单跟踪</text>
    </view>
    <view class='flex otherRow'>
      <view>
        <text>xx移动终端优化</text>
      </view>
      <view>
        <text>2018-01-01</text>
      </view>
    </view>
    <view class='flex otherRow'>
      <view>
        <text>xxx元</text>
      </view>
      <view>
        <text>由xx提交</text>
      </view>
    </view>
    <view>
      <text>带开票</text>
    </view>
  </view>
</view>
<view style='height:120rpx'></view>
<view class='addOrderBtn' data-kind='btn' bindtap='jump'>
  <button style='border-radius:0' class='ptButton' hover-class='ptButtonHover'>新增订单</button>
</view>