<!--pages/visit/ExternalContact/ExternalCon_record/ExternalCon_record.wxml-->
<view wx:for="{{fillList}}" class='row flex weui-check__label'>
  <view>
    <text>{{item.date}}</text>
  </view>
  <view>
    <view>
      <text>{{item.key1}}</text>
    </view>
    <view>
      <text>{{item.key2}}</text>
    </view>
  </view>
  <view class='flex'>
    <view bindtap='updataEXRecordBtn' class='icon'>
      <image src='../../../img/edit.png'></image>
    </view>
    <view class='icon'>
      <image src='../../../img/delete.png'></image>
    </view>
  </view>
</view>

<view>
  <button bindtap='newEXRecordBtn' class='ptButton ptButtonHover otherptButton'>新增拜访记录</button>
</view>