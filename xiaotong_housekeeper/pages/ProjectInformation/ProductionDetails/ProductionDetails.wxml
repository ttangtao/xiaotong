<!--pages/ProjectInformation/ProductionDetails/ProductionDetails.wxml-->
<view>
  <view wx:for="{{row1List}}" wx:key="{{index}}" class='flex row otherRow'>
    <text class='colorCls'>{{item.key}}</text>
    <text class='colorCls'>{{item.value}}</text>
  </view>
</view>

<view class='flex otherWDB'>
  <view>
    <text style='font-size:46rpx;color:#BA2835'>未待报</text>
  </view>
  <view style='line-height:30rpx'>
    <button size='mini' bindtap='jump' class='ptButton'>填报</button>
  </view>
</view>

<view>
  <view wx:for="{{row2List}}" wx:key="{{index}}" class='flex row otherRow'>
    <text>{{item.key}}</text>
    <text>{{item.value}}</text>
  </view>
</view>

<view style='padding:10rpx;' class='flex'>
  <view class='icon1'><image src='../../img/start2.png'></image></view>
  <view style='margin-left:10rpx;'><text>结算明细</text></view>
</view>

<view>
  <view wx:for="{{row3List}}" wx:key="{{index}}" class='flex row otherRow'>
    <text>{{item.key}}</text>
    <text>{{item.value}}</text>
  </view>
</view>
<view style='height:80rpx'></view>