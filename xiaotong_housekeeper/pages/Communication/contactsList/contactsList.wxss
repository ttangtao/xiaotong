@import "../Communication.wxss";
/*团队模块  */
.timeMenRow{
  background-color: #fff;
  padding: 20rpx;
  border-bottom: 1rpx solid #ccc;
  /* justify-content: space-between; */
}
.teamRow_headPic{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  background: #CD6A65;
}
.teamRow_info{
  margin-left: 20rpx;
  flex-grow: 1;
  font-size: 32rpx;
}
.teamRow_info_top,.teamRow_info_bottom{
  margin: 10rpx 0;
}
.teamRow_right{
  width: 35rpx;
  height: 45rpx;
  line-height: 45rpx;
}